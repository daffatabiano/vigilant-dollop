{
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext", "es2015"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["/src/*"],
            "@/components/*": ["components/*"]
        },
        "module": "esnext",

        "outDir": "./lib/cjs/",
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": true,
        "target": "es5",
        "moduleResolution": "node"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/**/*.ts*",
        "src/components/elements/Button/Button.jsx",
        "src/components/elements/Button/ButtonSectionContainer.jsx",
        "src/components/elements/Button/GroupButton.jsx",
        "src/components/elements/Input/Input.jsx",
        "src/components/elements/AccordionItems.jsx",
        "src/components/elements/AvatarsComment.jsx",
        "src/components/elements/CardBody.jsx",
        "src/components/elements/CardHead.jsx",
        "src/components/elements/DiscoverAside.jsx",
        "src/components/elements/DiscoverContain.jsx",
        "src/components/elements/FillCardContent.jsx",
        "src/components/elements/HambMenu.jsx",
        "src/components/elements/Header.jsx",
        "src/components/elements/LogoNav.jsx",
        "src/components/elements/PriceCard.jsx",
        "src/components/elements/SvgIcons.jsx",
        "src/components/Accordion.jsx",
        "src/components/Card.jsx",
        "src/components/CommentRate.jsx",
        "src/components/Modal.jsx",
        "src/components/NavList.jsx",
        "src/components/ProfilIcon.jsx",
        "src/components/Search.jsx",
        "src/components/SupportingFacilities.jsx",
        "src/fragments/sections/Discover.jsx",
        "src/fragments/sections/DiscoverFull.jsx",
        "src/fragments/sections/Explore.jsx",
        "src/fragments/sections/FAQ .jsx",
        "src/fragments/sections/Packages.jsx",
        "src/fragments/sections/PartnerSections.jsx",
        "src/fragments/Footer.jsx",
        "src/fragments/sections/Rating.jsx",
        "src/fragments/Navbar.jsx",
        "src/hooks/useAuth.jsx",
        "src/Layout/AuthLayout.jsx",
        "src/Layout/HomeLayout.jsx",
        "src/pages/Auth/login.jsx",
        "src/pages/_app.js",
        "src/pages/_document.js",
        "src/pages/index.jsx",
        "src/pages/Auth/api/[...nextauth].js",
        "jest.config.ts"
    ],
    "exclude": ["node_modules", "dist", "lib"]
}
